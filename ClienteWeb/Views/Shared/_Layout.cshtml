<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <!--Imagen con link-->
                <a class="navbar-brand" href="@Url.Action("Index","Home")">
                    <img src="https://yt3.ggpht.com/ytc/AKedOLQcsNSEZ3gweh0t-7sv4B12vGrn3-Xl_JFq41o_=s900-c-k-c0x00ffffff-no-rj" width="30" height="30" class="d-inline-block align-top" />
                </a>

                <div class="form-group">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Museo Atlantico", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })</li>
                        <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                        <li>@Html.ActionLink("Cuadros", "Obras", "Cuadro")</li>
                        <li>@Html.ActionLink("Artistas", "Artistas", "Autor")</li>
                        <li >
                            
                            <div class="btn-group">
                                <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                   <style>
                                       button{
                                           margin-top:8px;
                                       }
                                   </style>
                                    BUSCAR
                                </button>
                                <div class="dropdown-menu dropdown-menu-left" style="background-image: url(data:image/jpeg;base64,) ">          
                                    <a class="dropdown-item" href="@Url.Action("ConsultasA","Autor")">Artistas</a><br />
                                    <a class="dropdown-item" href="@Url.Action("ConsultasC","Cuadro")">Cuadros</a><br />
                                </div>
                            </div>
                        </li>
                    </ul>                   
                </div>
            </div>
        </div>
    </header>
    <div class="container body-content">
        <!--Estilo del fondo-->
        <style>
            body {
                background: rgb(58,180,97);
                background: linear-gradient(90deg, rgba(58,180,97,1) 0%, rgba(29,150,253,1) 50%, rgba(252,176,69,1) 100%);
            }
        </style>

        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Ander Pardo Alonso</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
